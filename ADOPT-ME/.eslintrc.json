{
  // sets of rules to use
  // be as opinionated as you want\
  "extends": ["eslint:recommended", "prettier", "prettier/react"],
  // any plugin config
  "plugins": [],
  // parser config
  "parserOptions": {
    // import and export - such modules
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
